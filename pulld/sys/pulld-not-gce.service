[Unit]
Description=Skia systemd monitoring UI and pull service.
Wants=network-online.target
After=network-online.target

[Service]
ExecStart=/usr/local/bin/pulld-not-gce --port=:10114 --on_gce=false --log_dir=/var/log/logserver --resources_dir=/usr/local/share/pulld/ --log_dir=/var/log/logserver --installed_packages_file=/var/pulld/installed_packages.json --service_account_path=/home/chrome-bot/service-account.json
Restart=always
User=root
Group=root
LimitNOFILE=10000

[Install]
WantedBy=multi-user.target
