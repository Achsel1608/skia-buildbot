[Unit]
Description=Ingestion for perf
Wants=network-online.target
After=network-online.target

[Service]
ExecStart=/usr/local/bin/ingest  \
    --config_filename=/usr/local/share/skiaperf/perf_ingest.toml \
    --log_dir=/var/log/logserver
Restart=always
User=default
Group=default
LimitNOFILE=10000

[Install]
WantedBy=multi-user.target
