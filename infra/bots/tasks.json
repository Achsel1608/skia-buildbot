{
  "jobs": {
    "Infra-PerCommit": {
      "priority": 0.8,
      "tasks": [
        "Infra-PerCommit"
      ]
    }
  },
  "tasks": {
    "Infra-PerCommit": {
      "dimensions": [
        "pool:Skia",
        "os:Ubuntu",
        "gpu:none"
      ],
      "extra_args": [
        "--workdir",
        "../../..",
        "swarm_infra",
        "repository=<(REPO)",
        "buildername=Infra-PerCommit",
        "mastername=fake-master",
        "buildnumber=2",
        "slavename=fake-buildslave",
        "nobuildbot=True",
        "swarm_out_dir=${ISOLATED_OUTDIR}",
        "revision=<(REVISION)",
        "<(PATCH_STORAGE?\"patch_storage=<(PATCH_STORAGE)\":\"\")",
        "<(PATCH_STORAGE?\"patch_issue=<(ISSUE)\":\"\")",
        "<(PATCH_STORAGE?\"patch_set=<(PATCHSET)\":\"\")",
        "<(PATCH_STORAGE?\"patch_ref=refs/changes/<(ISSUE_SHORT)/<(ISSUE)/<(PATCHSET)\":\"\")"
      ],
      "isolate": "swarm_recipe.isolate",
      "priority": 0.8
    }
  }
}